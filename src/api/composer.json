{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "algolia/algoliasearch-client-php": "^3.4",
    "atk14/csv-reader": "^1.1",
    "backpack/backupmanager": "3.0.1",
    "backpack/crud": "4.1.*",
    "backpack/filemanager": "1.1.7",
    "backpack/langfilemanager": "^4.1",
    "backpack/logmanager": "^4.0",
    "backpack/permissionmanager": "^6.0",
    "bunnycdn/storage": "^2.0",
    "ckhandla94/backpack-import": "^1.0",
    "corbpie/bunny-cdn-api": "^1.9",
    "cviebrock/eloquent-sluggable": "^8.0",
    "deeplcom/deepl-php": "^1.8",
    "fruitcake/laravel-cors": "^2.0",
    "google-gemini-php/client": "^1.0",
    "grok-php/client": "^1.1",
    "jenssegers/imagehash": "^0.9.0",
    "laravel/framework": "^8.75",
    "laravel/octane": "^1.5",
    "laravel/sanctum": "^2.15.1",
    "laravel/scout": "^9.8",
    "laravel/socialite": "^5.6",
    "laravel/tinker": "^2.5",
    "league/csv": "^9.16.0",
    "maatwebsite/excel": "^3.1",
    "openai-php/client": "^0.10.1",
    "orchestra/parser": "^6.1",
    "parabellumkoval/backpack-articles": "0.2.x-dev",
    "parabellumkoval/backpack-banners": "dev-main",
    "parabellumkoval/backpack-feedback": "dev-main",
    "parabellumkoval/backpack-pages": "0.1.x-dev",
    "parabellumkoval/backpack-profile": "dev-main",
    "parabellumkoval/backpack-reviews": "0.2.x-dev",
    "parabellumkoval/backpack-settings": "0.2.x-dev",
    "parabellumkoval/backpack-store": "0.3.x-dev",
    "parabellumkoval/backpack-tag": "0.1.x-dev",
    "parabellumkoval/backpack-transactions": "0.1.x-dev",
    "parabellumkoval/backpack-vercel": "0.1.x-dev",
    "plakidan/monobank-pay": "^0.2.1",
    "rap2hpoutre/fast-excel": "^5.4",
    "sapientpro/image-comparator": "^1.2",
    "spatie/laravel-feed": "^4.2",
    "spatie/laravel-translatable": "5.1.0"
  },
  "require-dev": {
    "backpack/generators": "^3.1",
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "laracasts/generators": "^2.0",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^5.10",
    "phpunit/phpunit": "^9.5.10",
    "symfony/filesystem": "*"
  },
  "autoload": {
      "psr-4": {
          "App\\": "app/",
          "Database\\Factories\\": "database/factories/",
          "Database\\Seeders\\": "database/seeders/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  },
  "scripts": {
      "post-autoload-dump": [
          "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
          "@php artisan package:discover --ansi"
      ],
      "post-update-cmd": [
          "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
      ],
      "post-root-package-install": [
          "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
      ],
      "post-create-project-cmd": [
          "@php artisan key:generate --ansi"
      ]
  },
  "extra": {
      "laravel": {
          "dont-discover": [
            "backpack/permissionmanager"
          ]
      }
  },
  "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true,
      "platform": {
          "php": "8.1.11"
      },
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": []
}
